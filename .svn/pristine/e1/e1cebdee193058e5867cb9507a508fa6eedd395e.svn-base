*          This is the routine which handles get response from SIH Get Processor
*          and prepare the SNMP GET response for EMS.
* @param tlvArray contains pointer to TLV array.
*/
TBOOL sendRlcGenCountersSnmpGetResponse (
  IO TlvArray    *tlvArray
  )
{
  int  totalNumRow, totalNumCol, i=0, colmn=0;
  rlcGenCountersTableEntry * prevPtr = NULL;

  printTLV(tlvArray);

  /* Total no. of Colmns */
  totalNumCol = RLC_GEN_COUNTERS_MAX_COL;
  /* Total no. of Row = (No. of TLV)/(No. of Columns) */
  totalNumRow = tlvArray->numTlvs/totalNumCol;

  logInfo("Total No. of Row %d & Col %d", totalNumRow, totalNumCol);
  for (i=1;  i <= totalNumRow;  i++)
      {
      rlcGenCountersTableEntry *ptr=(rlcGenCountersTableEntry *)malloc(sizeof(rlcGenCountersTableEntry));

      clearBytes (ptr, sizeof(rlcGenCountersTableEntry));

      /* Fill Data in TLV */
      for(colmn = 1 ; colmn <= totalNumCol ; colmn++)
        {
        if (colmn EQ COLUMN_RLC_GEN_COUNTERS_INDEX)
           {
            ptr->index =  getUint32FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
        if (colmn EQ COLUMN_RLC_GEN_COUNTERS_NUMUECFG)
           {
            ptr->numUeCfg =  getUint32FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
        if (colmn EQ COLUMN_PDUSRECV)
           {
            ptr->pdusRecv =  getUint32FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
        if (colmn EQ COLUMN_PDUSSENT)
           {
            ptr->pdusSent = getUint32FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
        if (colmn EQ COLUMN_PDUSRETX)
           {
            ptr->pdusRetx = getUint32FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
        if (colmn EQ COLUMN_BYTESRECV)
           {
            ptr->bytesRecv = getUint32FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
        if (colmn EQ COLUMN_BYTESSENT)
           {
            ptr->bytesSent = getUint32FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
        if (colmn EQ COLUMN_UNEXPPDUSRECV)
           {
            ptr->unexpPdusRecv = getUint32FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
        if (colmn EQ COLUMN_ERRORPDUSRECV)
           {
            ptr->errorPdusRecv = getUint32FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
        if (colmn EQ COLUMN_PROTTIMEOUT)
           {
            ptr->protTimeOut = getUint32FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
        if (colmn EQ COLUMN_NUMOFRB)
           {
            ptr-> numOfRb = getUint32FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
        if (colmn EQ COLUMN_NUMSDUDISC)
           {
            ptr-> numSduDisc = getUint32FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
	if (colmn EQ COLUMN_RLC_GEN_COUNTERS_LASTCOLLECTEDAT)
           {
            clearBytes (ptr->lastCollectedAt, RLC_GEN_COUNTERS_LCOLLECT_BUF_LEN_MAX);
            copyBytes (tlvArray->tlvInfo[colmn-1].data, tlvArray->tlvInfo[colmn-1].len, ptr->lastCollectedAt);
            ptr->lastCollectedAt_len = strlen(ptr->lastCollectedAt);
            continue;
           }
        }

       ptr->next = NULL;

      if (prevPtr == NULL) {
           rlcGenCountersTable_head =  prevPtr = ptr;
       }

      prevPtr->next = ptr;
      prevPtr = ptr;
     }
    prevPtr->next = NULL;

    if (rlcGenCountersTable_head)
        return TRUE;
    else
        return FALSE;
}


