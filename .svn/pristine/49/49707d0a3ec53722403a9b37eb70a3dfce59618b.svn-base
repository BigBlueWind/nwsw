            case COLUMN_TPDUEXCDTX:
                if ( !table_entry ) {
                    netsnmp_set_request_error(reqinfo, request,
                                              SNMP_NOSUCHINSTANCE);
                    continue;
                }
                snmp_set_var_typed_integer( request->requestvb, ASN_COUNTER,
                                            table_entry->tpduExcdTx);
                break;
            case COLUMN_INVDATTYPETX:
                if ( !table_entry ) {
                    netsnmp_set_request_error(reqinfo, request,
                                              SNMP_NOSUCHINSTANCE);
                    continue;
                }
                snmp_set_var_typed_integer( request->requestvb, ASN_COUNTER,
                                            table_entry->invDatTypeTx);
                break;
	    case COLUMN_GTP_GEN_COUNTERS_LASTCOLLECTEDAT:
                if ( !table_entry ) {
                    netsnmp_set_request_error(reqinfo, request,
                                              SNMP_NOSUCHINSTANCE);
                    continue;
                }
                snmp_set_var_typed_value( request->requestvb, ASN_OCTET_STR,
                                          table_entry->lastCollectedAt,
                                          table_entry->lastCollectedAt_len);
                break;
            default:
                netsnmp_set_request_error(reqinfo, request,
                                          SNMP_NOSUCHOBJECT);
                break;
            }
        }
	if(g_gtpGenCountersFirstCall EQ 1)
         {
            g_gtpGenCountersFirstCall = 0;
            g_prevGtpGenCountersReqCount = loopCount;
            // This will occur in case of, when SNMP-GET or SNMP-GETNEXT of Single Parameter occurred.
            // In the first call itself loopCount becomes one.
            // In General Case, the loopCount will be always > 1 because every table contains 
            // index column to fetch the row and remaining columns contain table attributes.
            if(loopCount EQ 1)
            {
               //Setting it to false in case of SNMP-GET request for single parameter.
               g_sihGtpGenCountersGetReqProcessed = FALSE;
               // Free all resources.
               free_gtpGenCountersTable();
            }
         }
         else if(loopCount > 1 || g_prevGtpGenCountersReqCount > 1)
         {
            //update g_prevReqCount with current loopCount for next call of Table_handler
            g_prevGtpGenCountersReqCount = loopCount;
         }
         else if(loopCount EQ 1)
         {
            //Setting it to false in case of SNMP-GET request for single parameter.
            g_sihGtpGenCountersGetReqProcessed = FALSE;
            // Free all resources.
            free_gtpGenCountersTable();
         }
        break;

    }
    return SNMP_ERR_NOERROR;
}
