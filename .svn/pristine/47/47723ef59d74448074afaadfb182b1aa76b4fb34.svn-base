                table_entry->intraFreqNeighCellQOffsetCell     = table_entry->old_intraFreqNeighCellQOffsetCell;
                table_entry->old_intraFreqNeighCellQOffsetCell = 0;
                break;
            }
        }
#endif /* PWAV_SIH */
        break;

    case MODE_SET_COMMIT:
        break;
    }
    return SNMP_ERR_NOERROR;
}
