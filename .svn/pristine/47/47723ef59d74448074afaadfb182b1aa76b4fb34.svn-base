        break;

    case MODE_SET_RESERVE2:
        break;

    case MODE_SET_FREE:
        break;

    case MODE_SET_ACTION:
        g_sihEutraNeighCellConfigSetReqProcessed = FALSE;

        clearBytes(&sihTagInfo, sizeof(TagInfo));
        clearBytes(&tlvArray, sizeof(TlvArray));
        
        for (request=requests; request; request=request->next) {
            table_entry = (struct eutraNeighCellConfigTable_entry *)
                              netsnmp_extract_iterator_context(request);
            table_info  =     netsnmp_extract_table_info(      request);

            if(table_entry == NULL){
                logInfo(" %s\n", "table_entry is NULL");
                if (reqinfo->mode == MODE_SET_ACTION) {
                    netsnmp_set_request_error(reqinfo, request,SNMP_NOSUCHINSTANCE);
                    continue;
                }
                /* XXX: no row existed, if you support creation and this is a
                   set, start dealing with it here, else continue */
            }

            if(table_info == NULL){
                logInfo(" %s\n", "table_info is NULL");
                continue;
            }

            colId = table_info->colnum;
            /* set the row index */
            rowIndex = requests->requestvb->name_loc[OID_ROW_POSITION];

            switch (table_info->colnum) {
            case COLUMN_NEIBOURINDEX:
                table_entry->neibourIndex     = *request->requestvb->val.integer;
                fillTableInfo(&tlvArray.tlvInfo[tlvArray.numTlvs].tag, EUTRANEIGHCELLCONFIG, rowIndex, COLUMN_NEIBOURINDEX);
                tlvArray.numTlvs++;
                break;
            case COLUMN_ISUSED:
                table_entry->isUsed     = *request->requestvb->val.integer;
                fillTableInfo(&tlvArray.tlvInfo[tlvArray.numTlvs].tag, EUTRANEIGHCELLCONFIG, rowIndex, COLUMN_ISUSED);
                tlvArray.numTlvs++;
                break;
            case COLUMN_NEIGHEUTRAENBID:
                table_entry->neighEutraEnbId     = *request->requestvb->val.integer;
                fillTableInfo(&tlvArray.tlvInfo[tlvArray.numTlvs].tag, EUTRANEIGHCELLCONFIG, rowIndex, COLUMN_NEIGHEUTRAENBID);
                tlvArray.numTlvs++;
                break;
            case COLUMN_NEIGHEUTRACELLID:
                table_entry->neighEutraCellId     = *request->requestvb->val.integer;
                fillTableInfo(&tlvArray.tlvInfo[tlvArray.numTlvs].tag, EUTRANEIGHCELLCONFIG, rowIndex, COLUMN_NEIGHEUTRACELLID);
                tlvArray.numTlvs++;
                break;
            case COLUMN_NEIGHEUTRAPHYCELLID:
                table_entry->neighEutraPhyCellId     = *request->requestvb->val.integer;
                fillTableInfo(&tlvArray.tlvInfo[tlvArray.numTlvs].tag, EUTRANEIGHCELLCONFIG, rowIndex, COLUMN_NEIGHEUTRAPHYCELLID);
                tlvArray.numTlvs++;
                break;
            case COLUMN_NEIGHEUTRAPLMNMCC:
                memset( table_entry->neighEutraPlmnMcc, 0,
                        sizeof(table_entry->neighEutraPlmnMcc));
                memcpy( table_entry->neighEutraPlmnMcc,
                        request->requestvb->val.string,
                        request->requestvb->val_len);
                table_entry->neighEutraPlmnMcc_len =
                        request->requestvb->val_len;
                fillTableInfo(&tlvArray.tlvInfo[tlvArray.numTlvs].tag, EUTRANEIGHCELLCONFIG, rowIndex, COLUMN_NEIGHEUTRAPLMNMCC);
                tlvArray.numTlvs++;
                break;
            case COLUMN_NEIGHEUTRAPLMNMNC:
                memset( table_entry->neighEutraPlmnMnc, 0,
                        sizeof(table_entry->neighEutraPlmnMnc));
                memcpy( table_entry->neighEutraPlmnMnc,
