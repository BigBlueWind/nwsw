                        request->requestvb->val_len;
                fillTableInfo(&tlvArray.tlvInfo[tlvArray.numTlvs].tag, EUTRANEIGHCELLCONFIG, rowIndex, COLUMN_NEIGHEUTRAPLMNMNC);
                tlvArray.numTlvs++;
                break;
            case COLUMN_NEIGHEUTRATAC:
                table_entry->neighEutraTac     = *request->requestvb->val.integer;
                fillTableInfo(&tlvArray.tlvInfo[tlvArray.numTlvs].tag, EUTRANEIGHCELLCONFIG, rowIndex, COLUMN_NEIGHEUTRATAC);
                tlvArray.numTlvs++;
                break;
            case COLUMN_NEIGHCELLMEASBANDWIDTH:
                table_entry->neighCellMeasBandwidth     = *request->requestvb->val.integer;
                fillTableInfo(&tlvArray.tlvInfo[tlvArray.numTlvs].tag, EUTRANEIGHCELLCONFIG, rowIndex, COLUMN_NEIGHCELLMEASBANDWIDTH);
                tlvArray.numTlvs++;
                break;
            case COLUMN_CELLINDIVIDUALOFFSET:
                table_entry->cellIndividualOffset     = *request->requestvb->val.integer;
                fillTableInfo(&tlvArray.tlvInfo[tlvArray.numTlvs].tag, EUTRANEIGHCELLCONFIG, rowIndex, COLUMN_CELLINDIVIDUALOFFSET);
                tlvArray.numTlvs++;
                break;
            case COLUMN_INTRAFREQNEIGHCELLQOFFSETCELL:
                table_entry->intraFreqNeighCellQOffsetCell     = *request->requestvb->val.integer;
                fillTableInfo(&tlvArray.tlvInfo[tlvArray.numTlvs].tag, EUTRANEIGHCELLCONFIG, rowIndex, COLUMN_INTRAFREQNEIGHCELLQOFFSETCELL);
                tlvArray.numTlvs++;
                break;
            }
        }

        if ((request == NULL) && (g_sihEutraNeighCellConfigSetReqProcessed EQ FALSE))
            /* Processed all requests and table entry is filled with
             * requested data. */
            {
            /* Set SIH Set Request Tag Info */
            sihTagInfo.tableId = EUTRANEIGHCELLCONFIG;

            sihTagInfo.rowId = rowIndex; 

            status = sendEutraNeighCellConfigSnmpSetRequest (sihTagInfo, table_entry, &tlvArray);
            if (status EQ TRUE)
            {
                logInfo("%s","SET Succedded");
            }
            else
                netsnmp_set_request_error(reqinfo, requests, SNMP_ERR_GENERR );
            }
        break;

    case MODE_SET_UNDO:
#ifndef PWAV_SIH 
        for (request=requests; request; request=request->next) {
            table_entry = (struct eutraNeighCellConfigTable_entry *)
                              netsnmp_extract_iterator_context(request);
            table_info  =     netsnmp_extract_table_info(      request);
    
            switch (table_info->colnum) {
            case COLUMN_NEIBOURINDEX:
                table_entry->neibourIndex     = table_entry->old_neibourIndex;
                table_entry->old_neibourIndex = 0;
                break;
            case COLUMN_ISUSED:
