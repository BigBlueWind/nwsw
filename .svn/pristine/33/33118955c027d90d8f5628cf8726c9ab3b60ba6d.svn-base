/* free_componentStatusTable */
/**
* @details 
*          This is the routine which frees the component status entry.
*/
void free_componentStatusTable(void)
{
  struct componentStatusTable_entry *current = componentStatusTable_head;
  struct componentStatusTable_entry *temp = NULL;
  
  if(current)
  {
    while(current->next != NULL)
    {
        temp = current->next;
        SNMP_FREE( current );
        current = temp;
    }

    SNMP_FREE( current );
    componentStatusTable_head = NULL;
  }
  return;
}


/**
* @details 
*          This is the routine which handles get response from SIH Get Processor
*          and prepare the SNMP GET response for EMS.
* @param tlvArray contains pointer to TLV array.
*/
TBOOL sendComponentStatusSnmpGetResponse (
  IO TlvArray    *tlvArray
  )
{
  int  totalNumRow, totalNumCol, i=0, colmn=0, pos=0;
  struct componentStatusTable_entry * prevPtr = NULL;

  printTLV(tlvArray);

  /* Total no. of Colmns */
  totalNumCol = COMPONENT_STATUS_MAX_COL;
  /* Total no. of Row = (No. of TLV)/(No. of Columns) */
  totalNumRow = tlvArray->numTlvs/totalNumCol;

  logInfo("%s","Total No. of Row %d & Col %d", totalNumRow, totalNumCol);
  for (i=1;  i <= totalNumRow;  i++)
      {
      struct componentStatusTable_entry *ptr=(struct componentStatusTable_entry *)malloc(sizeof(struct componentStatusTable_entry));

      clearBytes (ptr, sizeof(struct componentStatusTable_entry));

      /* Fill Data in TLV */
      for(colmn = 1 ; colmn <= totalNumCol ; colmn++, pos++)
        {
        if (colmn EQ COLUMN_COMPONENT_STATUS_INDEX)
           {
            ptr->index =  getUint32FromArray(tlvArray->tlvInfo[pos].data);
            continue;
           }
        if (colmn EQ COLUMN_COMPONENTTYPE)
           {
            ptr->componentType =  getUint32FromArray(tlvArray->tlvInfo[pos].data);
            continue;
           }
        if (colmn EQ COLUMN_COMPONENTVERSION)
           {
            clearBytes (ptr->componentVersion, COMPONENT_STATUS_VERSION_BUFFER_LEN_MAX);
            copyBytes (tlvArray->tlvInfo[pos].data, tlvArray->tlvInfo[pos].len, ptr->componentVersion);
            ptr->componentVersion_len = strlen(ptr->componentVersion);
            continue;
           }
        if (colmn EQ COLUMN_COMPONENTINSTANCE)
           {
            ptr->componentInstance =  getUint8FromArray(tlvArray->tlvInfo[pos].data);
            continue;
           }
        if (colmn EQ COLUMN_COMPONENTOPERATIONALSTATE)
           {
            ptr->componentOperationalState =  getUint32FromArray(tlvArray->tlvInfo[pos].data);
            continue;
           }
        if (colmn EQ COLUMN_INFOSTRING)
           {
            clearBytes (ptr->infoString, COMPONENT_STATUS_INFO_BUFFER_LEN_MAX);
            copyBytes (tlvArray->tlvInfo[pos].data, tlvArray->tlvInfo[pos].len, ptr->infoString);
            ptr->infoString_len = strlen(ptr->infoString);
            continue;
           }
        if (colmn EQ COLUMN_ERRORSTRING)
           {
            clearBytes (ptr->errorString, COMPONENT_STATUS_ERROR_BUFFER_LEN_MAX);
            copyBytes (tlvArray->tlvInfo[pos].data, tlvArray->tlvInfo[pos].len, ptr->errorString);
            ptr->errorString_len = strlen(ptr->errorString);
            continue;
           }
        }

       ptr->next = NULL;

      if (prevPtr == NULL) {
           componentStatusTable_head =  prevPtr = ptr;
       }

      prevPtr->next = ptr;
      prevPtr = ptr;
     }
    prevPtr->next = NULL;

    if (componentStatusTable_head)
        return TRUE;
    else
        return FALSE;
}


