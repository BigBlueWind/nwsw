/**
* @details 
*          This is the routine which handles get response from SIH Get Processor
*          and prepare the SNMP GET response for EMS. 
* @param tlvArray contains pointer to TLV array.     
*/
TBOOL sendEnbConfigSnmpGetResponse (
  IO TlvArray    *tlvArray
  )
{
  int  totalNumRow, totalNumCol, i=0, colmn=0;
  enbConfigTableEntry * prevPtr = NULL;

  printTLV(tlvArray);

  /* Total no. of Colmns */
  totalNumCol = ENODEB_CONFIG_MAX_COL;
  /* Total no. of Row = (No. of TLV)/(No. of Columns) */
  totalNumRow = tlvArray->numTlvs/totalNumCol;

  for (i=1;  i <= totalNumRow;  i++) 
      {
      enbConfigTableEntry *ptr=(enbConfigTableEntry *)malloc(sizeof(enbConfigTableEntry));

      clearBytes(ptr,sizeof(enbConfigTableEntry));

      /* Fill Data in TLV */
      for(colmn = 1 ; colmn <= totalNumCol ; colmn++)
        {
       if (colmn EQ COLUMN_ENODEBID)
           {
            ptr->enodebId = getUint32FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }        
       if (colmn EQ COLUMN_ENODEBNAME)
           {
            clearBytes (ptr->enodebName, ENODEB_BUFFER_LEN_MAX);
            if (tlvArray->tlvInfo[colmn-1].len > 0) 
                {    
                 copyBytes (tlvArray->tlvInfo[colmn-1].data, tlvArray->tlvInfo[colmn-1].len, ptr->enodebName);
                }
            ptr->enodebName_len = strlen(ptr->enodebName);
            continue;
           }
	if (colmn EQ COLUMN_USESTATICIPADDRESS)
	   {
	    ptr->useStaticIpAddress = getUint8FromArray(tlvArray->tlvInfo[colmn-1].data);
	    continue;		
	   }        
        if (colmn EQ COLUMN_ENODEBSTATICIPADDR)
           {
            ptr->enodebStaticIpAddr = inet_addr(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }        
        if (colmn EQ COLUMN_ENODEBNETMASKADDR)
           {
            ptr->enodebNetmaskAddr = inet_addr(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }        
        if (colmn EQ COLUMN_ENBREGISTRATIONACCEPTED)
           {
            ptr->enbRegistrationAccepted = getUint8FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
        if (colmn EQ COLUMN_CONFIGCHANGEACKBYEMS)
           {
            ptr->configChangeAckByEms = getUint8FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
        }

       ptr->next = NULL;

      if (prevPtr == NULL) {
           enbConfigTable_head =  prevPtr = ptr;
       }

      prevPtr->next = ptr;
      prevPtr = ptr;
     }
    prevPtr->next = NULL;

    if (enbConfigTable_head)
        return TRUE;
    else
        return FALSE;
}

/**
* @details 
*          This routine sends enodeb config set request to BSM, it uses SIH Set 
*          Processor to create and send the message to BSM. 
* @param sihTagInfo is used to have the tag information for set request.
*/
TBOOL sendEnbConfigSnmpSetRequest ( 
      IO TagInfo   sihTagInfo,
      IO TlvArray *tlvArray
      )
{
   enbConfigTableEntry * setReqData = NULL;   
   TBOOL status = FALSE;
   int colmn = 0;
   unsigned char * ipAddr;
   struct in_addr ip;
   
   setReqData = enbConfigTable_head;

   /* Fill Data in TLV */
   for(colmn = 0; colmn < tlvArray->numTlvs; colmn++)
      {
         if(tlvArray->tlvInfo[colmn].tag.colId EQ COLUMN_ENODEBID)
         {
           putUint32InArray( setReqData->enodebId, tlvArray->tlvInfo[colmn].data );
           tlvArray->tlvInfo[colmn].len = sizeof(setReqData->enodebId);     
         }
         if(tlvArray->tlvInfo[colmn].tag.colId EQ COLUMN_ENODEBNAME)
         {
           copyBytes (setReqData->enodebName, strlen(setReqData->enodebName), tlvArray->tlvInfo[colmn].data);
           tlvArray->tlvInfo[colmn].len = ENODEB_BUFFER_LEN_MAX-1; 
         }
         if(tlvArray->tlvInfo[colmn].tag.colId EQ COLUMN_USESTATICIPADDRESS)
         {
           putUint8InArray( setReqData->useStaticIpAddress, tlvArray->tlvInfo[colmn].data );
           tlvArray->tlvInfo[colmn].len = sizeof(setReqData->useStaticIpAddress);
         }	
         if(tlvArray->tlvInfo[colmn].tag.colId EQ COLUMN_ENODEBSTATICIPADDR)
         {
           ip.s_addr = setReqData->enodebStaticIpAddr;
           ipAddr = inet_ntoa(ip);
           copyBytes (ipAddr, strlen(ipAddr), tlvArray->tlvInfo[colmn].data);
           tlvArray->tlvInfo[colmn].len = ENODEBSTATICIPADDR_BUF_LEN_MAX; 
         }       
         if(tlvArray->tlvInfo[colmn].tag.colId EQ COLUMN_ENODEBNETMASKADDR)
         {
           ip.s_addr = setReqData->enodebNetmaskAddr;
           ipAddr = inet_ntoa(ip);
           copyBytes (ipAddr, strlen(ipAddr), tlvArray->tlvInfo[colmn].data);
           tlvArray->tlvInfo[colmn].len = ENODEBSTATICIPADDR_BUF_LEN_MAX;
         }       
         if(tlvArray->tlvInfo[colmn].tag.colId EQ COLUMN_ENBREGISTRATIONACCEPTED)
         {
           putUint8InArray( setReqData->enbRegistrationAccepted, tlvArray->tlvInfo[colmn].data );
           tlvArray->tlvInfo[colmn].len = sizeof(setReqData->enbRegistrationAccepted);     
         }          
         if(tlvArray->tlvInfo[colmn].tag.colId EQ COLUMN_CONFIGCHANGEACKBYEMS)
         {
           putUint8InArray( setReqData->configChangeAckByEms, tlvArray->tlvInfo[colmn].data );
           tlvArray->tlvInfo[colmn].len = sizeof(setReqData->configChangeAckByEms);
         }
      }

   /* Invoke SIH Set Processor */
   status = sihSetProcessor (sihTagInfo,  tlvArray);
   if (status EQ TRUE)
     {
      g_sihEnbConfigSetReqProcessed = TRUE;
      g_sihEnbConfigGetReqProcessed = FALSE;
      /* free the resources */      
      free_enbConfigTable(); 
     }
   return status;
}


