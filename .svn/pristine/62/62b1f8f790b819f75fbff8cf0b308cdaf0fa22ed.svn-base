            table_info  =     netsnmp_extract_table_info(request);

            if(table_entry == NULL){
                logInfo(" %s\n", "table_entry is NULL");
                if (reqinfo->mode == MODE_SET_ACTION) {
                    netsnmp_set_request_error(reqinfo, request,SNMP_NOSUCHINSTANCE);
                    continue;
                }
                /* XXX: no row existed, if you support creation and this is a
                   set, start dealing with it here, else continue */
            }

            if(table_info == NULL){
                logInfo(" %s\n", "table_info is NULL");
                continue;
            }
    
            colId = table_info->colnum;
            /* set the row index */
            rowIndex = requests->index;

            switch (table_info->colnum) {
            case COLUMN_ENODEBID:
                table_entry->enodebId     = *request->requestvb->val.integer;
                fillTableInfo(&tlvArray.tlvInfo[tlvArray.numTlvs].tag, ENODEBCONFIG, rowIndex, COLUMN_ENODEBID);
                tlvArray.numTlvs++;
                break;
            case COLUMN_ENODEBNAME:
                memset( table_entry->enodebName, 0,
                        sizeof(table_entry->enodebName));
                memcpy( table_entry->enodebName,
                        request->requestvb->val.string,
                        request->requestvb->val_len);
                table_entry->enodebName_len =
                        request->requestvb->val_len;
                fillTableInfo(&tlvArray.tlvInfo[tlvArray.numTlvs].tag, ENODEBCONFIG, rowIndex, COLUMN_ENODEBNAME);
                tlvArray.numTlvs++;
                break;
            case COLUMN_USESTATICIPADDRESS:
                table_entry->useStaticIpAddress     = *request->requestvb->val.integer;
                fillTableInfo(&tlvArray.tlvInfo[tlvArray.numTlvs].tag, ENODEBCONFIG, rowIndex, COLUMN_USESTATICIPADDRESS);
                tlvArray.numTlvs++;
                break;
            case COLUMN_ENODEBSTATICIPADDR:
                table_entry->enodebStaticIpAddr     = *request->requestvb->val.integer;
                fillTableInfo(&tlvArray.tlvInfo[tlvArray.numTlvs].tag, ENODEBCONFIG, rowIndex, COLUMN_ENODEBSTATICIPADDR);
                tlvArray.numTlvs++;
                break;
            case COLUMN_ENODEBNETMASKADDR:
                table_entry->enodebNetmaskAddr     = *request->requestvb->val.integer;
                fillTableInfo(&tlvArray.tlvInfo[tlvArray.numTlvs].tag, ENODEBCONFIG, rowIndex, COLUMN_ENODEBNETMASKADDR);
                tlvArray.numTlvs++;
                break;
            case COLUMN_ENBREGISTRATIONACCEPTED:
                table_entry->enbRegistrationAccepted = *request->requestvb->val.integer;
                fillTableInfo(&tlvArray.tlvInfo[tlvArray.numTlvs].tag, ENODEBCONFIG, rowIndex, COLUMN_ENBREGISTRATIONACCEPTED);
                tlvArray.numTlvs++;
                break;
            case COLUMN_CONFIGCHANGEACKBYEMS:
                table_entry->configChangeAckByEms  = *request->requestvb->val.integer;
                fillTableInfo(&tlvArray.tlvInfo[tlvArray.numTlvs].tag, ENODEBCONFIG, rowIndex, COLUMN_CONFIGCHANGEACKBYEMS);
                tlvArray.numTlvs++;
                break;
            }
        }

        if ((request == NULL) && (g_sihEnbConfigSetReqProcessed EQ FALSE))
            /* Processed all requests and table entry is filled with
             * requested data. */
            {
            /* Set SIH Set Request Tag Info */
            sihTagInfo.tableId = ENODEBCONFIG;

            sihTagInfo.rowId = rowIndex; 

            status = sendEnbConfigSnmpSetRequest (sihTagInfo, &tlvArray);
            if (status EQ TRUE)
            {
                logInfo("%s","SET Succedded");
            }
            else
                netsnmp_set_request_error(reqinfo, requests, SNMP_ERR_GENERR );
            }
        break;

    case MODE_SET_UNDO:
#ifndef PWAV_SIH 
        for (request=requests; request; request=request->next) {
            table_entry = (struct enbConfigTable_entry *)
                              netsnmp_extract_iterator_context(request);
            table_info  =     netsnmp_extract_table_info(      request);
    
            switch (table_info->colnum) {
            case COLUMN_ENODEBID:
                table_entry->enodebId     = table_entry->old_enodebId;
                table_entry->old_enodebId = 0;
                break;
            case COLUMN_ENODEBNAME:
                memcpy( table_entry->enodebName,
                        table_entry->old_enodebName,
                        sizeof(table_entry->enodebName));
                memset( table_entry->old_enodebName, 0,
                        sizeof(table_entry->enodebName));
                table_entry->enodebName_len =
                        table_entry->old_enodebName_len;
                break;
            case COLUMN_PAGINGDRXCYCLE:
                table_entry->pagingDrxCycle     = table_entry->old_pagingDrxCycle;
                table_entry->old_pagingDrxCycle = 0;
                break;
            case COLUMN_MMEIPADDR:
                table_entry->mmeIpAddr     = table_entry->old_mmeIpAddr;
                table_entry->old_mmeIpAddr = 0;
                break;
            case COLUMN_SECUREMMELINK:
                table_entry->secureMmeLink     = table_entry->old_secureMmeLink;
                table_entry->old_secureMmeLink = 0;
                break;
            case COLUMN_SECURESGWLINK:
                table_entry->secureSgwLink     = table_entry->old_secureSgwLink;
                table_entry->old_secureSgwLink = 0;
                break;
            case COLUMN_ENODEBSTATICIPADDR:
                table_entry->enodebStaticIpAddr     = table_entry->old_enodebStaticIpAddr;
                table_entry->old_enodebStaticIpAddr = 0;
                break;
            case COLUMN_SCTPNUMINBOUNDSTREAMS:
                table_entry->sctpNumInboundStreams     = table_entry->old_sctpNumInboundStreams;
                table_entry->old_sctpNumInboundStreams = 0;
                break;
            case COLUMN_SCTPNUMOUTBOUNDSTREAMS:
                table_entry->sctpNumOutboundStreams     = table_entry->old_sctpNumOutboundStreams;
                table_entry->old_sctpNumOutboundStreams = 0;
                break;
            case COLUMN_ACTIVECELLID:
                table_entry->activeCellId     = table_entry->old_activeCellId;
                table_entry->old_activeCellId = 0;
                break;
            case COLUMN_UERESPONSETIMER:
                table_entry->ueResponseTimer     = table_entry->old_ueResponseTimer;
                table_entry->old_ueResponseTimer = 0;
                break;
            case COLUMN_RRCCONNREJWAITTIME:
                table_entry->rrcConnRejWaitTime     = table_entry->old_rrcConnRejWaitTime;
                table_entry->old_rrcConnRejWaitTime = 0;
                break;
            case COLUMN_LOCALTIME:
                memcpy( table_entry->localTime,
                        table_entry->old_localTime,
                        sizeof(table_entry->localTime));
                memset( table_entry->old_localTime, 0,
                        sizeof(table_entry->localTime));
                table_entry->localTime_len =
                        table_entry->old_localTime_len;
                break;
            case COLUMN_LOCALTIMEOFFSETFROMUTC:
                memcpy( table_entry->localTimeOffsetFromUTC,
                        table_entry->old_localTimeOffsetFromUTC,
                        sizeof(table_entry->localTimeOffsetFromUTC));
                memset( table_entry->old_localTimeOffsetFromUTC, 0,
                        sizeof(table_entry->localTimeOffsetFromUTC));
                table_entry->localTimeOffsetFromUTC_len =
                        table_entry->old_localTimeOffsetFromUTC_len;
                break;
            }
        }
#endif /* PWAV_SIH */
        break;

    case MODE_SET_COMMIT:
        break;
    }
    return SNMP_ERR_NOERROR;
}
