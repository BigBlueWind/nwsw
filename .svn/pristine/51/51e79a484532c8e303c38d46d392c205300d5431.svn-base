/* free_swInstallTable */
/**
* @details 
*          This is the routine which frees the sw install entry.
*/
void free_swInstallTable(void)
{
  swInstallTableEntry  * current = swInstallTable_head;

  if (current)
  {
  for (current = swInstallTable_head->next ; current != NULL; current = current->next) {
    SNMP_FREE( current );
    }

  SNMP_FREE(swInstallTable_head);
  swInstallTable_head = NULL;
  }
  return;
}


/**
* @details 
*          This is the routine which handles get response from SIH Get Processor
*          and prepare the SNMP GET response for EMS.
* @param tlvArray contains pointer to TLV array.
*/
TBOOL sendSwInstallSnmpGetResponse (
  IO TlvArray    *tlvArray
  )
{
  int  totalNumRow, totalNumCol, i=0, colmn=0;
  swInstallTableEntry * prevPtr = NULL;

  printTLV(tlvArray);

  logInfo("%s","sendSwInstallSnmpGetResponse");
  /* Total no. of Colmns */
  totalNumCol = SW_INSTALL_MAX_COL;
  /* Total no. of Row = (No. of TLV)/(No. of Columns) */
  totalNumRow = tlvArray->numTlvs/totalNumCol;

  logInfo("Total No. of Row %d & Col %d", totalNumRow, totalNumCol);
  for (i=1;  i <= totalNumRow;  i++)
      {
      swInstallTableEntry *ptr=(swInstallTableEntry *)malloc(sizeof(swInstallTableEntry));

      clearBytes (ptr, sizeof(swInstallTableEntry));

      /* Fill Data in TLV */
      for(colmn = 1 ; colmn <= totalNumCol ; colmn++)
        {
        if (colmn EQ COLUMN_SWINSTALLINDEX)
           {
            ptr->index =  getUint32FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
        if (colmn EQ COLUMN_INSTALLATIONTYPE)
           {
            ptr->installationType =  getUint32FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
        if (colmn EQ COLUMN_SWINSTALLFILENAME)
           {
            clearBytes (ptr->fileName, SWINSTALLFILENAME_BUF_LEN);
            if (tlvArray->tlvInfo[colmn-1].len > 0)
            {
                copyBytes (tlvArray->tlvInfo[colmn-1].data, tlvArray->tlvInfo[colmn-1].len, ptr->fileName);
            }
            ptr->fileName_len = strlen(ptr->fileName);
            continue;
           }
        if (colmn EQ COLUMN_INSTALLER)
           {
            clearBytes (ptr->installer, SWINSTALLFILENAME_BUF_LEN);
            if (tlvArray->tlvInfo[colmn-1].len > 0)
            {
                copyBytes (tlvArray->tlvInfo[colmn-1].data, tlvArray->tlvInfo[colmn-1].len, ptr->installer);
            }
            ptr->installer_len = strlen(ptr->installer);
            continue;
           }
        }

       ptr->next = NULL;

      if (prevPtr == NULL) {
           swInstallTable_head =  prevPtr = ptr;
       }

      prevPtr->next = ptr;
      prevPtr = ptr;
     }
    prevPtr->next = NULL;

    if (swInstallTable_head)
        return TRUE;
    else
        return FALSE;
}


/**
* @details 
*          This routine sends file download set request to BSM, it uses SIH Set
*          Processor to create and send the message to BSM.
* @param sihTagInfo is used to have the tag information for set request.
*/
TBOOL sendSwInstallSnmpSetRequest (
      TagInfo  sihTagInfo
      )
{
   TlvArray tlvArray;
   swInstallTableEntry * setReqData = NULL;
   TBOOL status = FALSE;
   int colmn = 0, maxCols = 0;

   clearBytes(&tlvArray,sizeof(tlvArray));

   setReqData = swInstallTable_head;

  /* max columns in row */
   maxCols = SW_INSTALL_MAX_COL;

   /* Set the Tags */
   if (sihTagInfo.colId)
   {
    /* Tag for setting an element in table */
    tlvArray.tlvInfo[0].tag = sihTagInfo;
   }
   else
   {
   /* Tag for setting a row in table */
   for(colmn = 1 ; colmn <= maxCols ; colmn++)
      {
      tlvArray.tlvInfo[colmn-1].tag.tableId  = sihTagInfo.tableId;
      tlvArray.tlvInfo[colmn-1].tag.rowId    = sihTagInfo.rowId;
      tlvArray.tlvInfo[colmn-1].tag.colId    = colmn;
      }
    }

   /* Fill Data in TLV */
   for(colmn = 1; colmn <= maxCols ; colmn++)
      {
       if (((sihTagInfo.colId EQ CONFIG_COL_ID_NONE) && (colmn EQ COLUMN_SWINSTALLINDEX)) ||
           (sihTagInfo.colId EQ COLUMN_SWINSTALLINDEX))
           {
           putUint32InArray( setReqData->index, tlvArray.tlvInfo[colmn-1].data );
           tlvArray.tlvInfo[colmn-1].len = sizeof(setReqData->index);
           tlvArray.numTlvs++;
           if (sihTagInfo.colId EQ COLUMN_SWINSTALLINDEX)
               break;
           }
       if (((sihTagInfo.colId EQ CONFIG_COL_ID_NONE) && (colmn EQ COLUMN_INSTALLATIONTYPE)) ||
           (sihTagInfo.colId EQ COLUMN_INSTALLATIONTYPE))
           {
           putUint32InArray( setReqData->installationType, tlvArray.tlvInfo[colmn-1].data );
           tlvArray.tlvInfo[colmn-1].len = sizeof(setReqData->installationType);
           tlvArray.numTlvs++;
           if (sihTagInfo.colId EQ COLUMN_INSTALLATIONTYPE)
               break;
           }
       if (((sihTagInfo.colId EQ CONFIG_COL_ID_NONE) && (colmn EQ COLUMN_SWINSTALLFILENAME)) ||
           (sihTagInfo.colId EQ COLUMN_SWINSTALLFILENAME))
           {
           clearBytes (tlvArray.tlvInfo[colmn-1].data, sizeof(tlvArray.tlvInfo[colmn-1].data));
           copyBytes (setReqData->fileName, strlen(setReqData->fileName),
                      tlvArray.tlvInfo[colmn-1].data);
           tlvArray.tlvInfo[colmn-1].len = SWINSTALLFILENAME_BUF_LEN-1;
           tlvArray.numTlvs++;
           if (sihTagInfo.colId EQ COLUMN_SWINSTALLFILENAME)
               break;
           }
       if (((sihTagInfo.colId EQ CONFIG_COL_ID_NONE) && (colmn EQ COLUMN_INSTALLER)) ||
           (sihTagInfo.colId EQ COLUMN_INSTALLER))
           {
           clearBytes (tlvArray.tlvInfo[colmn-1].data, sizeof(tlvArray.tlvInfo[colmn-1].data));
           copyBytes (setReqData->installer, strlen(setReqData->installer),
                      tlvArray.tlvInfo[colmn-1].data);
           tlvArray.tlvInfo[colmn-1].len = SWINSTALLFILENAME_BUF_LEN-1;
           tlvArray.numTlvs++;
           if (sihTagInfo.colId EQ COLUMN_INSTALLER)
               break;
           }
       }

   /* Invoke SIH Set Processor */
   status = sihSetProcessor (sihTagInfo,  &tlvArray);
   if (status EQ TRUE)
     {
      g_sihSwInstallSetReqProcessed = TRUE;
      g_sihSwInstallGetReqProcessed = FALSE;
      /* free the resources */
      free_swInstallTable();
     }

   return status;
}


