$(document).ready(function() {
var fn = function(e) {
return DisableRefresh(e);
}
document.onkeydown = fn;
$(document).bind('contextmenu',             function(e) {              alert('Due to security reason, Right Click is not allowed.'); return false; });
LteGet('enbStatus', null, SetMapping);
var interval = '';
$('#rfrshInt').change(function() {
 if ($('#rfrshInt').val() != 'NONE') {
if (interval != '')
clearInterval(interval);
interval = setInterval('LteGet("enbStatus", null, SetMapping)', $('#rfrshInt').val() * 1000);
}
else
clearInterval(interval);
});
$('#btnHelp').click(function() {
window.open(sessionID + "-help.htm#enbStatus", '_blank', "width=800,height=600,status=no,toolbar=no,menubar=no,directories=no,location=no,resizable=no");
});
});
