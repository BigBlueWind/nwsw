<html>
<head>
<style type="text/css" lang="css">
@import '../css/styles.css';
    </style>
<script type="text/javascript" src="../script/jquery.js"></script>
<script type="text/javascript" src="../script/lte_set_get.js"></script>
<script type="text/javascript" src="../script/mapping.js"></script>
<script type="text/javascript">
    var sessionID = parent.sessionID;
function addRowStatus(tableID, sno, data)
{
    var table = parent.frames["MiddleRight"].document.getElementById(tableID);
    if (table != null) {
        var rowCount = table.rows.length;
        table.width = "100%";
        table.border = "0";
        table.cellpadding = "0";
        table.cellspacing = "0";
        table.overflow = "auto";
        if (data.col[0].value == "" || data.col[0].value == "0" || data.col[0].value == "None") 
        {
            if (sno == 1) {
                var row = table.insertRow(rowCount);
                var cell1 = row.insertCell(0);
                cell1.align = "center";
                cell1.width = "100%"
                cell1.innerHTML = "<span class='errormsg'>No File Event History Present.</span>";
            }
        }
        else {
            var row = table.insertRow(rowCount);
            var cell1 = row.insertCell(0);
            var cell2 = row.insertCell(1);
            var cell3 = row.insertCell(2);
            var cell4 = row.insertCell(3);
            if (rowCount % 2 == 1) {
                row.className = "conTblRowBg2"
            }
            else {
                row.className = "conTblRowBg1"
            }

            cell1.align = "center";
            cell1.width = "8%"
            cell1.innerHTML = data.col[0].value;

            cell2.align = "center";
            cell2.width = "50%"
            cell2.innerHTML = data.col[1].value;

            cell3.align = "center";
            cell3.width = "25%";
            cell3.innerHTML = SetMultiRowMapping("fileEventHistory", data.col[2].param, data.col[2].value);
            

            cell4.align = "center";
            cell4.width = "17%";
            cell4.innerHTML = SetMultiRowMapping("fileEventHistory", data.col[3].param, data.col[3].value);
            

        }
    }
}

$(document).ready(function() {
var fn = function(e) {
    return DisableRefresh(e);
}
document.onkeydown = fn;
document.onkeydown = fn;
$(document).bind("contextmenu", function(e) { alert("Due to security reason,Right Click is not allowed."); return false; });   
    var interval = "";
    updateTable("fileEventHistory", "fileEventHistoryTable");
    $("#rfrshInt").change(function() {
        if ($("#rfrshInt").val() != "NONE") {
            if (interval != "")
                clearInterval(interval);
            interval = setInterval('updateTable("fileEventHistory", "fileEventHistoryTable")', $("#rfrshInt").val() * 1000);
        }
        else
            clearInterval(interval);
    });
    $('#btnHelp').click(function() {
    window.open(sessionID + "-help.htm#fileEventHistory", '_blank', "width=800,height=600,status=no,toolbar=no,menubar=no,directories=no,location=no,resizable=no");
    });
});
</script>

</head>
<body>
    <table width="100%">
        <tr>
            <td align="left" width="60%">
                <span class="title">Monitor &gt; File Event History</span>
            </td>
            <td align="right" width="40%">
                <span class="cmdMsg" id="cmdMsg"></span>
            </td>
        </tr>
    </table>
<table width='100%'  border='0' cellspacing='0' cellpadding='0'><tr><td align='left' valign='top' class='horline'><img src='../image/horline.gif' width='1' height='1'></td></tr>
</table>
    <div style=' margin-top: 10px; left: 10px;'>
            <table cellspacing="0" cellpadding="2" border="0" width="1055" style="margin-top: 10px">
                <tbody>
                    <tr>
                     <td align="left" style="font-size:smaller;">&nbsp;&nbsp;&nbsp;Automatic Refresh Interval (Seconds)&nbsp;&nbsp; 
                        :&nbsp;
                        <select name="rfrshInt" id="rfrshInt">
                            <option value="NONE">NONE</option>
                            <option value="5">5</option>
                            <option value="10">10</option>
                            <option value="15">15</option>
                            <option value="30">30</option>
                            <option value="60">60</option>
                        </select></td>
                        <td align="right" class="txtPageTitle">
                            <input type='button' value='Help' name='btnHelp' id='btnHelp' />
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>          
    <div style="margin-left: 10px; margin-top: 10px; width: 1040px;">
    <form id="fileEventHistory" action=''>
        <table width='100%' class='tblBorder' cellspacing='0' cellpadding='0' border='0'>
            <thead>
                <tr>
                    <td align='center' width='8%' class='tblTitleBg'>
                        S.No
                    </td>
                    <td align='center' width='50%' class='tblTitleBg'>
                        File Name
                    </td>
                    <td align='center' width='25%' class='tblTitleBg'>
                        Status
                    </td>
                    <td align='center' width='17%' class='tblTitleBg'>
                        Successful
                    </td>                    
                </tr>
                <tr class="formheadingborder">
                    <td colspan="4">
                    </td>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td colspan="4" valign='top' align='center'>
                        <div style="width: 100%;">                    
                            <table id="fileEventHistoryTable" width="100%" border="0" cellpadding="0" cellspacing="0">
                                <thead>
                                    <tr>
                                        <th align='center'  width='8%' nowrap="nowrap">
                                        </th>
                                        <th align='center'  width='50%' nowrap="nowrap">
                                        </th>
                                        <th align='center'  width='25%' nowrap="nowrap">
                                        </th>
                                        <th align='center'  width='17%' nowrap="nowrap">
                                        </th>                                       
                                    </tr>
                                </thead>
                            </table>                   
                        </div>
                    </td>
                </tr>
            </tbody>
        </table>
         </form>
     </div>
</body>
</html>
