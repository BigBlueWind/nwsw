/* free_traceControlTable */
/**
* @details 
*          This is the routine which frees the trace control entry.
*/
void free_traceControlTable(void)
{
  struct traceControlTable_entry *current = traceControlTable_head;

  if (current)
  {
        for (current = traceControlTable_head->next ; current != NULL; current = current->next) {
                SNMP_FREE( current );
        }

        SNMP_FREE(traceControlTable_head);
        traceControlTable_head = NULL;
  }
  return;
}


/**
* @details 
*          This is the routine which handles get response from SIH Get Processor
*          and prepare the SNMP GET response for EMS.
* @param tlvArray contains pointer to TLV array.
*/
TBOOL sendTraceControlSnmpGetResponse (
  IO TlvArray    *tlvArray
  )
{
  int  totalNumRow, totalNumCol, i=0, colmn=0;
  struct traceControlTable_entry * prevPtr = NULL;

  printTLV(tlvArray);

  /* Total no. of Colmns */
  totalNumCol = TRACE_CONTROL_MAX_COL;
  /* Total no. of Row = (No. of TLV)/(No. of Columns) */
  totalNumRow = tlvArray->numTlvs/totalNumCol;

  logInfo("Total No. of Row %d & Col %d", totalNumRow, totalNumCol);
  for (i=1;  i <= totalNumRow;  i++)
      {
      struct traceControlTable_entry *ptr=(struct traceControlTable_entry *)malloc(sizeof(struct traceControlTable_entry));

      clearBytes (ptr, sizeof(struct traceControlTable_entry));

      /* Fill Data in TLV */
      for(colmn = 1 ; colmn <= totalNumCol ; colmn++)
        {
        if (colmn EQ COLUMN_TRACEINDEX)
           {
            ptr->index =  getUint32FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
        if (colmn EQ COLUMN_TRACECOMMAND)
           {
            ptr->traceCommand =  getUint32FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
        if (colmn EQ COLUMN_TRACEDURATION)
           {
            ptr->traceDuration =  getUint32FromArray(tlvArray->tlvInfo[colmn-1].data);
            continue;
           }
        }

       ptr->next = NULL;

      if (prevPtr == NULL) {
           traceControlTable_head =  prevPtr = ptr;
       }

      prevPtr->next = ptr;
      prevPtr = ptr;
     }
    prevPtr->next = NULL;

    if (traceControlTable_head)
        return TRUE;
    else
        return FALSE;
}


/**
* @details 
*          This routine sends enodeb config set request to BSM, it uses SIH Set
*          Processor to create and send the message to BSM.
* @param sihTagInfo is used to have the tag information for set request.
*/
TBOOL sendTraceControlSnmpSetRequest (
      TagInfo  sihTagInfo
      )
{
   TlvArray tlvArray;
   traceControlTableEntry * setReqData = NULL;
   TBOOL status = FALSE;
   int colmn = 0, maxCols = 0;

   clearBytes(&tlvArray,sizeof(tlvArray));

   setReqData = traceControlTable_head;

  /* max columns in row */
   maxCols = TRACE_CONTROL_MAX_COL;

   /* Set the Tags */
   if (sihTagInfo.colId)
   {
    /* Tag for setting an element in table */
    tlvArray.tlvInfo[0].tag = sihTagInfo;
   }
   else
   {
   /* Tag for setting a row in table */
   for(colmn = 1 ; colmn <= maxCols ; colmn++)
      {
      tlvArray.tlvInfo[colmn-1].tag.tableId  = sihTagInfo.tableId;
      tlvArray.tlvInfo[colmn-1].tag.rowId    = sihTagInfo.rowId;
      tlvArray.tlvInfo[colmn-1].tag.colId    = colmn;
      }
    }

   /* Fill Data in TLV */
   for(colmn = 1; colmn <= maxCols ; colmn++)
      {
       if (((sihTagInfo.colId EQ CONFIG_COL_ID_NONE) &&
           (colmn EQ COLUMN_TRACEINDEX)) ||
           (sihTagInfo.colId EQ COLUMN_TRACEINDEX))
           {
           putUint32InArray( setReqData->index, tlvArray.tlvInfo[colmn-1].data );
           tlvArray.tlvInfo[colmn-1].len = sizeof(setReqData->index);
           tlvArray.numTlvs++;
           if (sihTagInfo.colId EQ COLUMN_TRACEINDEX)
               break;
           }
       if (((sihTagInfo.colId EQ CONFIG_COL_ID_NONE) &&
           (colmn EQ COLUMN_TRACECOMMAND)) ||
           (sihTagInfo.colId EQ COLUMN_TRACECOMMAND))
           {
           putUint32InArray( setReqData->traceCommand, tlvArray.tlvInfo[colmn-1].data );
           tlvArray.tlvInfo[colmn-1].len = sizeof(setReqData->traceCommand);
           tlvArray.numTlvs++;
           if (sihTagInfo.colId EQ COLUMN_TRACECOMMAND)
               break;
           }
       if (((sihTagInfo.colId EQ CONFIG_COL_ID_NONE) &&
           (colmn EQ COLUMN_TRACEDURATION)) ||
           (sihTagInfo.colId EQ COLUMN_TRACEDURATION))
           {
           putUint32InArray( setReqData->traceDuration, tlvArray.tlvInfo[colmn-1].data );
           tlvArray.tlvInfo[colmn-1].len = sizeof(setReqData->traceDuration);
           tlvArray.numTlvs++;
           if (sihTagInfo.colId EQ COLUMN_TRACEDURATION)
               break;
           }
       }

   /* Invoke SIH Set Processor */
   status = sihSetProcessor (sihTagInfo,  &tlvArray);
   if (status EQ TRUE)
     {
      g_sihTraceControlSetReqProcessed = TRUE;
      g_sihTraceControlGetReqProcessed = FALSE;
      /* free the resources */
      free_traceControlTable();
     }
   return status;
}


