#ifndef RESULTCODES_H_
#define RESULTCODES_H_

#include "basetypes.h"

//
// Common return codes
//
#define SUCCESS                     0
#define FAILURE                     1
#define EXPIRED                     2       // Not an error - wait operation expired
#define RESTART                     3       // Not an error - indicate we need to restart process

//
// Fatal errors
//
#define ERR_EXIT                    4
#define ERR_FATAL2                  5

#define ERR_INVSIZE                 6       // Invalid size
#define ERR_INVTYPE                 7       // Invalid type
#define ERR_NOMEM                   8       // No memory (unspecified)
#define ERR_NOHEAPMEM               9       // No heap memory
#define ERR_NOPARTMEM               10      // No partition (fast array) memory
#define ERR_QUEFULL                 11      // Queue full

