/* free_msgTraceTable */
/**
* @details 
*          This is the routine which frees the enodeb config entry. 
*/
void free_msgTraceTable(void)
{
  msgTraceTableEntry  *current = msgTraceTable_head;
  msgTraceTableEntry  *temp = NULL;
  
  if(current)
  {
    while(current->next != NULL)
    {
        temp = current->next;
        SNMP_FREE( current );
        current = temp;
    }

    SNMP_FREE( current );
    msgTraceTable_head = NULL;
  }
  return;
}


/**
* @details 
*          This is the routine which handles get response from SIH Get Processor
*          and prepare the SNMP GET response for EMS. 
* @param tlvArray contains pointer to TLV array.     
*/
TBOOL sendMsgTraceSnmpGetResponse (
  IO TlvArray    *tlvArray
  )
{
  int  totalNumRow, totalNumCol, i=0, colmn=0, pos=0;
  msgTraceTableEntry * prevPtr = NULL;

  printTLV(tlvArray);

  /* Total no. of Colmns */
  totalNumCol = MSG_TRACE_MAX_COL;
  /* Total no. of Row = (No. of TLV)/(No. of Columns) */
  totalNumRow = tlvArray->numTlvs/totalNumCol;

  for (i=1;  i <= totalNumRow;  i++) 
      {
      msgTraceTableEntry *ptr=(msgTraceTableEntry *)malloc(sizeof(msgTraceTableEntry));

      clearBytes(ptr,sizeof(msgTraceTableEntry));

      /* Fill Data in TLV */
      for(colmn = 1 ; colmn <= totalNumCol ; colmn++, pos++)
        {
       if (colmn EQ COLUMN_MSGID)
           {
            ptr->msgId = getUint32FromArray(tlvArray->tlvInfo[pos].data);
            continue;
           }        
       if (colmn EQ COLUMN_MSGTRACELEVEL)
           {
            ptr->msgTraceLevel = getUint32FromArray(tlvArray->tlvInfo[pos].data);
            continue;
           }        
       if (colmn EQ COLUMN_MSGTRACEDIRECTION)
           {
            ptr->msgTraceDirection = getUint32FromArray(tlvArray->tlvInfo[pos].data);
            continue;
           }
        }

       ptr->next = NULL;

      if (prevPtr == NULL) {
           msgTraceTable_head =  prevPtr = ptr;
       }

      prevPtr->next = ptr;
      prevPtr = ptr;
     }
    prevPtr->next = NULL;

    if (msgTraceTable_head)
        return TRUE;
    else
        return FALSE;
}

/**
* @details 
*          This routine sends enodeb config set request to BSM, it uses SIH Set 
*          Processor to create and send the message to BSM. 
* @param sihTagInfo is used to have the tag information for set request.
*/
TBOOL sendMsgTraceSnmpSetRequest ( 
      TagInfo  sihTagInfo,
      struct msgTraceTable_entry  *msgTrace_entry
      )
{
   TlvArray tlvArray;
   msgTraceTableEntry * setReqData = NULL;   
   TBOOL status = FALSE;
   int colmn = 0, maxCols = 0;
   unsigned char * ipAddr;
   struct in_addr ip;
 
   clearBytes(&tlvArray,sizeof(tlvArray));
    
   setReqData = msgTrace_entry;

  /* max columns in row */
   maxCols = MSG_TRACE_MAX_COL;

   /* Set the Tags */
   if (sihTagInfo.colId)
   {
    /* Tag for setting an element in table */
    tlvArray.tlvInfo[0].tag = sihTagInfo;
   }
   else
   {
   /* Tag for setting a row in table */
   for(colmn = 1 ; colmn <= maxCols ; colmn++)
      {
      tlvArray.tlvInfo[colmn-1].tag.tableId  = sihTagInfo.tableId;
      tlvArray.tlvInfo[colmn-1].tag.rowId    = sihTagInfo.rowId;
      tlvArray.tlvInfo[colmn-1].tag.colId    = colmn;
      }
    }

   /* Fill Data in TLV */
   for(colmn = 1; colmn <= maxCols ; colmn++)
      {
       if (((sihTagInfo.colId EQ CONFIG_COL_ID_NONE) &&
           (colmn EQ COLUMN_MSGID)) || 
           (sihTagInfo.colId EQ COLUMN_MSGID))
           {
           putUint32InArray( setReqData->msgId, tlvArray.tlvInfo[colmn-1].data );
           tlvArray.tlvInfo[colmn-1].len = sizeof(setReqData->msgId);     
           tlvArray.numTlvs++;           
           if (sihTagInfo.colId EQ COLUMN_MSGID)
               break;
           }
       if (((sihTagInfo.colId EQ CONFIG_COL_ID_NONE) &&
           (colmn EQ COLUMN_MSGTRACELEVEL)) || 
           (sihTagInfo.colId EQ COLUMN_MSGTRACELEVEL))
           {
           putUint32InArray( setReqData->msgTraceLevel, tlvArray.tlvInfo[colmn-1].data );
           tlvArray.tlvInfo[colmn-1].len = sizeof(setReqData->msgTraceLevel);  
           tlvArray.numTlvs++;
           if (sihTagInfo.colId EQ COLUMN_MSGTRACELEVEL)
               break;
           }
       if (((sihTagInfo.colId EQ CONFIG_COL_ID_NONE) &&
           (colmn EQ COLUMN_MSGTRACEDIRECTION)) || 
           (sihTagInfo.colId EQ COLUMN_MSGTRACEDIRECTION))
           {
           putUint32InArray( setReqData->msgTraceDirection, tlvArray.tlvInfo[colmn-1].data );
           tlvArray.tlvInfo[colmn-1].len = sizeof(setReqData->msgTraceDirection); 
           tlvArray.numTlvs++;
           if (sihTagInfo.colId EQ COLUMN_MSGTRACEDIRECTION)
               break;
           }
       }

   /* Invoke SIH Set Processor */
   status = sihSetProcessor (sihTagInfo,  &tlvArray);
   if (status EQ TRUE)
     {
      g_sihMsgTraceSetReqProcessed = TRUE;
      g_sihMsgTraceGetReqProcessed = FALSE;
      /* free the resources */      
      free_msgTraceTable(); 
     }
   return status;
}


