<html>
<head>
<meta http-equiv="content-type" content="text/html; charset=windows-1252">
<style type="text/css" lang="css">
@import '../css/styles.css';
    </style>
<script type="text/javascript" src="../script/jquery.js"></script>
<script type="text/javascript" src="../script/lte_set_get.js"></script>
<script type="text/javascript" src="../script/mapping.js"></script>
<script type="text/javascript" src="../script/validate.js"></script>
<script type="text/javascript">
    var sessionID = parent.sessionID;
    var startTime = "";
    var endTime = "";
    var query = "";
    var rand = "";
    var table = "";
    var change = 0;
    var numOfRows = 0;
    $(document).ready(function() {
        table = document.getElementById("timersConfigTable");
        rand = (new Date()).valueOf();
        query = "key=timersConfig&rand=" + rand;
        $("#btnApply").click(function() {
            var msg = "";
            change = 0;
            for (i = 0; i < table.rows.length; i++) {
                msg = msg + IntRangeChk("duration" + (i + 1), "Duration" + (i + 1), 0, 429496729);
            }
            if (msg != "") {
                alert(msg);
            }
            else {
                SetTimer(table.rows.length);
            }
        });

        startTime = new Date();
        GetTimer();
        $('#btnHelp').click(function() {
        window.open(sessionID + "-help.htm#timersConfig", '_blank', "width=800,height=600,status=no,toolbar=no,menubar=no,directories=no,location=no,resizable=no");
        });
    });
    function GetTimer() {
        $.post(sessionID + "-LTEWebGet.fcgi", query,
           function(response) {
               endTime = new Date();
               if (response != null || response != undefined) {
                   if (response.result == "SUCCESS") {
                       numOfRows = response.data.row.length;
                       var rowCount = table.rows.length;
                       for (var i = 0, j = 0; j < rowCount; j++) {
                           table.deleteRow(i);
                       }
                       rowCount = table.rows.length;                       
                       table.width = "1005px";
                       table.border = "0";
                       table.cellpadding = "0";
                       table.cellspacing = "0";
                       table.overflow = "auto"
                       for (i = 0; i < response.data.row.length; i++) {
                           rowCount = table.rows.length;
                           var row = table.insertRow(rowCount);
                           if (rowCount % 2 == 1) {
                               row.className = "conTblRowBg2"
                           }
                           else {
                               row.className = "conTblRowBg1"
                           }

                           var cell1 = row.insertCell(0);
                           cell1.align = "center";
                           cell1.width = "5%";
                           cell1.innerHTML = i + 1;

                           var cell2 = row.insertCell(1);
                           cell2.align = "center";
                           cell2.width = "20%";
                           cell2.id = "timerName" + (i + 1);
                           cell2.innerHTML = response.data.row[i].col[0].value;

                           var cell3 = row.insertCell(2);
                           cell3.align = "center";
                           cell3.width = "10%";
                           var element3 = document.createElement("select");
                           element3.id = "timeUnit" + (i + 1);
                           addOption(element3, "Usec", 1);
                           addOption(element3, "Msec", 2);
                           addOption(element3, "Sec", 3);
                           addOption(element3, "Minute", 4);
                           addOption(element3, "Max", 5);
                           element3.value = response.data.row[i].col[1].value;
                           cell3.appendChild(element3);
                           $('body').append("<input type='hidden' id='timeUnitHidden" + (i + 1) + "' value='" + response.data.row[i].col[1].value + "' />");
                           
                           var cell4 = row.insertCell(3);
                           cell4.align = "center";
                           cell4.width = "10%";
                           var element4 = document.createElement("input");
                           element4.id = "duration" + (i + 1);
                           element4.size = "10";
                           element4.value = response.data.row[i].col[2].value;
                           cell4.appendChild(element4);
                           $('body').append("<input type='hidden' id='durationHidden" + (i + 1) + "' value='" + response.data.row[i].col[2].value + "' />");
                           
                           var cell5 = row.insertCell(4);
                           cell5.align = "center";
                           cell5.width = "10%";
                           cell5.id = "description" + (i + 1);
                           cell5.innerHTML = response.data.row[i].col[4].value;

                           var cell6 = row.insertCell(5);
                           cell6.align = "left";
                           cell6.width = "45%";
                           cell6.id = "usedBy" + (i + 1);
                           cell6.innerHTML = response.data.row[i].col[3].value;
                           SetMapping("timersConfig", (i + 1));
                       }
                       eTime = new Date();
                       STATUS("GET", "timersConfig", startTime, endTime, response.result, response.data);
                   }
                   else {
                       STATUS("GET", "timersConfig", startTime, endTime, response.result, response.data);
                   }
               }
               else {
                   parent.window.location.replace("lte_login.htm");
               }
           }
            , "json");

    }
    function SetTimer( requestnum )
    {
        if( requestnum > 0)
        {
            var i = numOfRows - requestnum;
            if (($("#durationHidden" + (i + 1)).val() == $("#duration" + (i + 1)).val()) && ($("#timeUnitHidden" + (i + 1)).val() == $("#timeUnit" + (i + 1)).val()) ) {
                requestnum--;
                SetTimer(requestnum);
                if (change == 0 && requestnum == 0) {
                    //$("#cmdMsg").html("No Fields Updated.......!").css("color", "red");
                    alert("Command NOT dispatched, as no parameter value is modified.");
                }
                return;
            }
            change++;
            $("#cmdMsg").html("");
            startTime = new Date();            
            request = "key=timersConfig&webGuiRow=" + (i + 1);
            if($("#durationHidden" + (i + 1)).val() != $("#duration" + (i + 1)).val())
                request = request + "&duration=" + $("#duration" + (i + 1)).val();
            if ($("#timeUnitHidden" + (i + 1)).val() != $("#timeUnit" + (i + 1)).val())
                request = request + "&timeUnit=" + $("#timeUnit" + (i + 1)).val();
            $.post(sessionID + "-LTEWebSet.fcgi", request,
                    function(response) {
                        endTime = new Date();
                        if (response != null && response != "undefined") {
                            if (response.result == "SUCCESS") {
                                for (var j = 0; j < response.data.row[0].col.length; j++) {
                                    if ($('#' + response.data.row[0].col[j].param + (i + 1)).is('input, select')) {
                                        $('#' + response.data.row[0].col[j].param + (i + 1)).val(response.data.row[0].col[j].value);
                                        $('#' + response.data.row[0].col[j].param + 'Hidden' + (i + 1)).val(response.data.row[0].col[j].value);
                                    }
                                    else if ($('#' + response.data.row[0].col[j].param + (i + 1)).is('td, text'))
                                        $('#' + response.data.row[0].col[j].param + (i + 1)).text(response.data.row[0].col[j].value);
                                }
                                STATUS('SET', "timersConfig", startTime, endTime, response.result, response.data);
                                requestnum--;
                                SetTimer(requestnum);
                            }
                            else {
                                STATUS('SET', "timersConfig", startTime, endTime, response.result, response.data);
                                SetTimer(0);
                            }
                        }
                        else {
                            STATUS('SET', "timersConfig", startTime, endTime, "FAIL", "INVALID DATA");
                            SetTimer(0);
                        }
                    }, "json");
           }
    }

    
</script>
</head>
<body>
    <table width="100%">
        <tr>
            <td align="left" width="60%">
                <span class="title">Configuration > Timers</span>
            </td>
            <td align="right" width="40%">
                <span class="cmdMsg" id="cmdMsg"></span>
            </td>
        </tr>
    </table>
    <table width='100%' border='0' cellspacing='0' cellpadding='0'><tr><td align='left' valign='top' class='horline'><img src='../image/horline.gif' width='1' height='1'/></td></tr>
    </table>
    <div style='margin-top: 10px; left: 10px;'>
        <table cellspacing="0" cellpadding="1" border="0" width="1055px">
            <tbody>
                <tr>
                    <td align="right" class="txtPageTitle">
                        <input type='button' value='Apply' name='btnApply' id='btnApply' />
                        <input type='button' value='Help' name='btnHelp' id='btnHelp' />
                    </td>
                </tr>
            </tbody>
        </table>
    </div>    
    <div style='margin-left: 10px;width:1020px;'>
        <form name='timersConfig' id='timersConfig' action=''>
        <table width='100%' class='tblBorder' cellspacing='0' cellpadding='0' border='0'>
            <thead>
                <tr>
                    <td align='center' width='5%' class='tblTitleBg'>
                        S.No
                    </td>
                    <td align='center' width='20%' class='tblTitleBg'>
                        Name
                    </td>
                    <td align='center' width='10%' class='tblTitleBg'>
                        Timer Unit
                    </td>
                    <td align='center' width='10%' class='tblTitleBg'>
                        Duration
                    </td>
                    <td align='center' width='10%' class='tblTitleBg'>
                        Used By
                    </td>
                    <td align='center' width='45%' class='tblTitleBg'>
                        Description
                    </td>
                </tr>
            </thead>
            <tbody>
                <tr  style="width:100%;height: 2px; background-color: #D2CEBC">
                         <td colspan='6'></td>             
                </tr>
                <tr>                
                    <td colspan="6" valign='top' align='center'>
                        <div style="width: 1020px; height:610px;overflow-y:scroll;overflow-x:hidden;scrollbar-base-color: rgb(155, 153, 207); scrollbar-arrow-color: black'">                            
                            <table id="timersConfigTable" width="100%" border="0" cellpadding="2" cellspacing="0">
                                <thead>
                                    <tr style="">
                                        <th align='center' width='5%' nowrap="nowrap">
                                        </th>
                                        <th align='center' width='20%' nowrap="nowrap">
                                        </th>
                                        <th align='center' width='10%' nowrap="nowrap">
                                        </th>
                                        <th align='center' width='10%' nowrap="nowrap">
                                        </th>
                                        <th align='center' width='10%' nowrap="nowrap">
                                        </th>
                                        <th align='center' width='45%' nowrap="nowrap">
                                        </th>
                                    </tr>
                                </thead>
                            </table>                           
                        </div>
                    </td>
                </tr>
            </tbody>
        </table>
         </form>
    </div>
</body>
</html>
