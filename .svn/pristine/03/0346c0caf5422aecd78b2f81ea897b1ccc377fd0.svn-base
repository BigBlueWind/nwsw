* @details 
*          This is the routine which frees the alarm history entry.
*/
void free_alarmHistoryTable(void)
{
  struct alarmHistoryTable_entry *current = alarmHistoryTable_head;
  struct alarmHistoryTable_entry *temp = NULL;

  if(current)
  {
    while(current->next != NULL)
    {
        temp = current->next;
        SNMP_FREE( current );
        current = temp;
    }

    SNMP_FREE( current );
    alarmHistoryTable_head = NULL;
  }
  return;
}


/**
* @details 
*          This is the routine which handles get response from SIH Get Processor
*          and prepare the SNMP GET response for EMS.
* @param tlvArray contains pointer to TLV array.
*/
TBOOL sendAlarmHistorySnmpGetResponse (
  IO TlvArray    *tlvArray
  )
{
  int  totalNumRow, totalNumCol, i=0, colmn=0, pos=0;
  struct alarmHistoryTable_entry * prevPtr = NULL;

  printTLV(tlvArray);

  if (tlvArray->numTlvs == 0)
  { return FALSE; }

  /* Total no. of Colmns */
  totalNumCol = ALARMHISTORY_MAX_COL;
  /* Total no. of Row = (No. of TLV)/(No. of Columns) */
  totalNumRow = tlvArray->numTlvs/totalNumCol;

  logInfo("%s","Total No. of Row %d & Col %d", totalNumRow, totalNumCol);
  for (i=1;  i <= totalNumRow;  i++)
      {
      struct alarmHistoryTable_entry *ptr=(struct alarmHistoryTable_entry *)malloc(sizeof(struct alarmHistoryTable_entry));

      clearBytes (ptr, sizeof(struct alarmHistoryTable_entry));

      /* Fill Data in TLV */
      for(colmn = 1 ; colmn <= totalNumCol ; colmn++, pos++)
        {
        if (colmn EQ COLUMN_ALARMHISTORY_INDEX)
           {
            ptr->index =  getUint32FromArray(tlvArray->tlvInfo[pos].data);
            continue;
           }
        if (colmn EQ COLUMN_ALARMHISTORY_ALARMNOTIFICATIONTYPE)
           {
            ptr->alarmNotificationType =  getUint32FromArray(tlvArray->tlvInfo[pos].data);
            continue;
           }
        if (colmn EQ COLUMN_ALARMHISTORY_NOTIFICATIONSEQUENCENUMBER)
           {
            ptr->notificationSequenceNumber =  getUint32FromArray(tlvArray->tlvInfo[pos].data);
            continue;
           }
        if (colmn EQ COLUMN_ALARMHISTORY_DATEANDTIMESTAMP)
           {
            clearBytes (ptr->dateAndTimeStamp, ALARMHISTORY_DATEANDTIMESTAMP_MAX_LEN);
            copyBytes (tlvArray->tlvInfo[pos].data, tlvArray->tlvInfo[pos].len, ptr->dateAndTimeStamp);
            ptr->dateAndTimeStamp_len = strlen(ptr->dateAndTimeStamp);
            continue;
           }
        if (colmn EQ COLUMN_ALARMHISTORY_ALARMTYPE)
           {
            ptr->alarmType =  getUint32FromArray(tlvArray->tlvInfo[pos].data);
            continue;
           }
        if (colmn EQ COLUMN_ALARMHISTORY_ALARMID)
           {
            ptr->alarmId =  getUint32FromArray(tlvArray->tlvInfo[pos].data);
            continue;
           }
        if (colmn EQ COLUMN_ALARMHISTORY_ALARMSEVERITY)
           {
            ptr->alarmSeverity =  getUint32FromArray(tlvArray->tlvInfo[pos].data);
            continue;
           }
        if (colmn EQ COLUMN_ALARMHISTORY_ALARMCAUSE)
           {
            ptr->alarmCause =  getUint32FromArray(tlvArray->tlvInfo[pos].data);
            continue;
           }
        if (colmn EQ COLUMN_ALARMHISTORY_ALARMLOCATION)
           {
            clearBytes (ptr->alarmLocation, ALARMHISTORY_ALARMLOCATION_MAX_LEN);
            copyBytes (tlvArray->tlvInfo[pos].data, tlvArray->tlvInfo[pos].len, ptr->alarmLocation);
            ptr->alarmLocation_len = strlen(ptr->alarmLocation);
            continue;
           }
        if (colmn EQ COLUMN_ALARMHISTORY_ALARMADDITIONALTEXT)
           {
            clearBytes (ptr->alarmAdditionalText, ALARMHISTORY_ALARMADDITIONALTEXT_MAX_LEN);
            copyBytes (tlvArray->tlvInfo[pos].data, tlvArray->tlvInfo[pos].len, ptr->alarmAdditionalText);
            ptr->alarmAdditionalText_len = strlen(ptr->alarmAdditionalText);
            continue;
           }
        }
       ptr->next = NULL;

      if (prevPtr == NULL) {
           alarmHistoryTable_head =  prevPtr = ptr;
       }

      prevPtr->next = ptr;
      prevPtr = ptr;
     }
    prevPtr->next = NULL;

    if (alarmHistoryTable_head)
        return TRUE;
    else
        return FALSE;
}


