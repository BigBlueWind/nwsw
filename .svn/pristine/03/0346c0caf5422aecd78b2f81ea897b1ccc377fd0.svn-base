    int loopCount = 0;

    DEBUGMSGTL(("alarmHistory:handler", "Processing request (%d)\n", reqinfo->mode));

    switch (reqinfo->mode) {
        /*
         * Read-support (also covers GetNext requests)
         */
    case MODE_GET:
        for (request=requests; request; request=request->next) {

            loopCount++;
            if (request->processed != 0) /*Processed all requests, rows and colmns */
                {
                g_prevAlarmHistoryReqCount = 0;
                g_sihAlarmHistoryGetReqProcessed = FALSE;
                /* free the resources */
                free_alarmHistoryTable();
