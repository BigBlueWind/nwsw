*          This is the routine which frees the mac counters entry.
*/
void free_fileEventHistoryTable(void)
{
  fileEventHistoryTableEntry  * current = fileEventHistoryTable_head;
  fileEventHistoryTableEntry  * temp = NULL;

  if(current)
  {
    while(current->next != NULL)
    {
	temp = current->next;
	SNMP_FREE( current );
	current = temp;
    }

    SNMP_FREE( current );
    fileEventHistoryTable_head = NULL;
  }
  return;
}


/**
* @details 
*          This is the routine which handles get response from SIH Get Processor
*          and prepare the SNMP GET response for EMS.
* @param tlvArray contains pointer to TLV array.
*/
TBOOL sendFileEventHistorySnmpGetResponse (
  IO TlvArray    *tlvArray
  )
{
  int  totalNumRow, totalNumCol, i=0, colmn=0, pos = 0;
  fileEventHistoryTableEntry * prevPtr = NULL;

  printTLV(tlvArray);

  /* Total no. of Colmns */
  totalNumCol = FILE_EVENT_HISTORY_MAX_COL;
  /* Total no. of Row = (No. of TLV)/(No. of Columns) */
  totalNumRow = tlvArray->numTlvs/totalNumCol;

  logInfo("Total No. of Row %d & Col %d", totalNumRow, totalNumCol);
  for (i=1;  i <= totalNumRow;  i++)
      {
      fileEventHistoryTableEntry *ptr=(fileEventHistoryTableEntry *)malloc(sizeof(fileEventHistoryTableEntry));

      clearBytes (ptr, sizeof(fileEventHistoryTableEntry));

      /* Fill Data in TLV */
      for(colmn = 1 ; colmn <= totalNumCol ; colmn++, pos++)
        {
        if (colmn EQ COLUMN_FILE_EVENT_HISTORY_INDEX)
           {
            ptr->index =  getUint32FromArray(tlvArray->tlvInfo[pos].data);
            continue;
           }
        if (colmn EQ COLUMN_FILE_EVENT_HISTORY_FILENAME)
           {
            clearBytes (ptr->fileName, FILE_EVENT_HISTORY_NAME_BUF_LEN_MAX);
            if (tlvArray->tlvInfo[pos].len > 0) 
                {    
                 copyBytes (tlvArray->tlvInfo[pos].data, tlvArray->tlvInfo[pos].len, ptr->fileName);
                }
            ptr->fileName_len = strlen(ptr->fileName);
            continue;
           }
        if (colmn EQ COLUMN_FILESTATUS)
           {
            ptr->fileStatus =  getUint32FromArray(tlvArray->tlvInfo[pos].data);
            continue;
           }
        if (colmn EQ COLUMN_WASSUCCESSFUL)
           {
            ptr->wasSuccessful =  getUint8FromArray(tlvArray->tlvInfo[pos].data);
            continue;
           }
        }

       ptr->next = NULL;

      if (prevPtr == NULL) {
           fileEventHistoryTable_head =  prevPtr = ptr;
       }

      prevPtr->next = ptr;
      prevPtr = ptr;
     }
    prevPtr->next = NULL;

    if (fileEventHistoryTable_head)
        return TRUE;
    else
        return FALSE;
}

